var students = ["никита", "еще никита", "еще никита1", "еще никита2", "еще никита3", "еще никита4", "еще никита5", "еще никита6", "еще никита7", "еще никита8", "еще никита9", "еще никита10", "дев1", "дев2", "дев3", "дев4", "дев5", "дев6", "дев7"]
print("Исходный список учеников:")
for student in students {
    print(student)
}

students.removeSubrange(0...6)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}
